<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>registrar</from-outcome>
            <to-view-id>/views/registrar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>qazStudio</from-outcome>
            <to-view-id>/views/qazStudio.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/views/registrar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>yolos</from-outcome>
            <to-view-id>/views/qazStudio.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/views/qazStudio.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>gameTable</from-outcome>
            <to-view-id>/views/games.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>movieTable</from-outcome>
            <to-view-id>/views/movies.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>itemTable</from-outcome>
            <to-view-id>/views/items.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/views/games.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>gameForm</from-outcome>
            <to-view-id>/views/forms/gameForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/views/forms/gameForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>yoloG</from-outcome>
            <to-view-id>/views/games.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/views/movies.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>movieForm</from-outcome>
            <to-view-id>/views/forms/movieForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/views/forms/movieForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>yoloM</from-outcome>
            <to-view-id>/views/movies.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/views/forms/itemForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>yoloI</from-outcome>
            <to-view-id>/views/items.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/views/items.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>itemForm</from-outcome>
            <to-view-id>/views/forms/itemForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
